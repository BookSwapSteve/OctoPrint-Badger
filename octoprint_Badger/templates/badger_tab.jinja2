<div class="row-fluid" >
    <h3>Badger</h3>
    <p data-bind="text: status"></p>
</div>

<!-- Who's Printing main tab contents when a user is logged in -->
<div data-bind="visible: loginStateViewModel.isUser">
    <h4>Do Not Hack Label</h4>
    <div>
        <button type="button" class="btn btn-success" data-bind="click: printDoNotHack">Print Do Not Hack</button>
    </div>

    <h4>How To Register Label</h4>
    <div>
        <button type="button" class="btn btn-success" data-bind="click: printHowToRegister">Print How To Register</button>
    </div>

    <h4>Text Labels</h4>
    <div>
        <div>
            <textarea cols="100" rows="8" data-bind="value:textBlock"></textarea>
        </div>
        <button type="button" class="btn btn-success" data-bind="click: printTextBlock">Print Text</button>
    </div>
</div>
<div class="row-fluid" >
    <h3>Badger</h3>
</div>

<!-- Who's Printing main tab contents when a user is logged in -->
<div data-bind="visible: loginStateViewModel.isUser">
    <p data-bind="text: status"></p>
    <div>
        <h4>Do Not Hack Label</h4>
        <button type="button" class="btn btn-success" data-bind="click: printDoNotHack">Print Do Not Hack</button>
    </div>


    <div>
        <h4>Members Box Label</h4>
        <button type="button" class="btn btn-success" data-bind="click: printMembersBox">Print Box Label</button>
    </div>


    <div>
        <h4>How To Register Label</h4>
        <button type="button" class="btn btn-success" data-bind="click: printHowToRegister">Print How To Register</button>
    </div>


    <div>
        <h4>Genera Text Label</h4>
        <div>
            <textarea cols="100" rows="8" data-bind="value:textBlock"></textarea>
        </div>
        <button type="button" class="btn btn-success" data-bind="click: printTextBlock">Print Text</button>
    </div>


    <div>
        <h3>Print Queue</h3>
        <ul data-bind="foreach: printJobs">
            <li data-bind="text: name"></li>
        </ul>
        <button type="button" class="btn btn-success" data-bind="click: getPrintQueue">Refresh</button>
    </div>

    <h3>Maintenance</h3>

    <div>
        <h4>Clear Print Queue</h4>
        <button type="button" class="btn btn-success" data-bind="click: clearPrintQueue">Clear Print Queue</button>
    </div>


    <div>
        <h4>Labels Refilled</h4>
        <button type="button" class="btn btn-success" data-bind="click: labelsRefilled">Labels Refilled</button>
    </div>
</div>